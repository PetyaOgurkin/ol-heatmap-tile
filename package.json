{
  "name": "ol-heatmap-tile",
  "version": "1.1.1",
  "description": "ol plugin for rendering grayscale images",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build:clean": "del cjs esm types umd",
    "build:cjs": "babel src --out-dir cjs --extensions .ts",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm --extensions .ts",
    "build:types": "tsc --emitDeclarationOnly",
    "build:umd": "cross-env BABEL_ENV=rollup NODE_ENV=development rollup -c",
    "build:umd:min": "cross-env BABEL_ENV=rollup NODE_ENV=production rollup -c",
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm && npm run build:types && npm run build:umd && npm run build:umd:min"
  },
  "keywords": [
    "heatmap",
    "interpolation",
    "gfs",
    "weather visualization",
    "ventusky",
    "windy",
    "ol"
  ],
  "repository": "github:PetyaOgurkin/ol-heatmap-tile",
  "author": "PetyaOgurkin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@types/proj4": "^2.5.2",
    "cross-env": "^7.0.3",
    "ol": "^6.13.0",
    "rollup": "^2.63.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "ol": "^6.9.0",
    "proj4": "^2.8.0"
  },
  "files": [
    "cjs/*",
    "esm/*",
    "types/*",
    "umd/*"
  ]
}
